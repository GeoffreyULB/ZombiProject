public class Actor {
  private int health;
  final private int healtMax = 200;
  private int posX;
  private int posY;
  
  public int getPosX() {
    return posX;
    }
    
  public int getPosY() {
    return posY;
    }
    
  public void move(int X, int Y) {
    this.posX = this.posX + X;
    this.posY = this.posY + Y;
    }
    
  public int getHealth() {
    return health; 
    }
    
  public int getHealthMax() {
    return healthMax;
    }
    
  public void setHealth(int health) {
    this.health = health
    }
